{
  "manifest_version": 3,
  "name": "GabaritKDP Tracker",
  "version": "1.2.0",
  "permissions": [
    "scripting",
    "tabs",
    "storage",
    "cookies"
  ],
  "host_permissions": [
    "https://kdpreports.amazon.com/*",
    "http://127.0.0.1:3001/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}